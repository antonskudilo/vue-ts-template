{
  "name": "vue-ts-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --config vite.config/index.ts",
    "build": "vite build --config vite.config/index.ts",
    "preview": "vite preview --config vite.config/index.ts",
    "lint": "eslint src --ext .ts,.vue",
    "format": "prettier --write .",
    "test:components": "jest --testPathPattern=tests/components --coverage --coverageDirectory=coverage/components",
    "test:unit": "jest --testPathPattern=tests/unit --coverage --coverageDirectory=coverage/unit",
    "test:integration": "jest --testPathPattern=tests/integration --coverage --coverageDirectory=coverage/integration",
    "test": "jest --coverage --coverageDirectory=coverage/all"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.6",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.0.0",
    "@vue/vue3-jest": "^29.0.0",
    "babel-jest": "^29.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.3.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "less": "^4.4.0",
    "less-plugin-glob": "^3.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  }
}
